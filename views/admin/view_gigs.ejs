
                <!-- Content Wrapper START -->
                <div class="main-content">
                    <div class="container-fluid">
                         <div class="row">
                            <div class="col-md-12">
                                <div class="card" style="border: solid 1px #787878;">
                                    <div class="card-block">
                                        <h4 class="card-title">GIGS</h4>                    
                                        <div class="table-overflow">
                                            <table class="table table-hover table-sm">
                                                <thead>
                                                    <tr>
                                                        <th><i style="color:#13CB22" class="ti-briefcase"></i> Title</th>
                                                        
                                                        <th><i style="color: #004CFF" class="ti-user"></i> 
                                                            Email of Owner</th>
                                                        <th><i style="color: #FF00C6" class="ti-calendar"></i> Main Category</th>
                                                        <th><i style="color: #FF00C6" class="ti-calendar"></i> Sub Category</th>
                                                        <th><i style="color: #FF6700" class="ti-agenda"></i> Actions</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                        <% for(var i=0; gigs[i]; i++){ %>
                                                    <tr>
                                                        <td><%= gigs[i].title %></td>
                                                        <td><%= gigs[i].email %></td>
                                                        <td><%= gigs[i].main_category %></td>
                                                        <td><%= gigs[i].sub_category %></td>
                                                        <td>
                                                            <div class="btn-group">
                                                                <a class="btn btn-primary" href="/to-switch/edit_gigs/<%= gigs[i].unique_id %>"><i class="c-red-500 ti-pencil"></i></a>
                                                                <% if(gigs[i].blocked == '1' ){ %>
                                                                <a data-toggle="modal" gig-id="<%= gigs[i].unique_id %>" class="btn btn-outline-danger" href="#myModullBLOCKGIG"><i class="ti-lock"></i>
                                                                </a>
                                                                <% } else if(gigs[i].blocked == '0'){ %>
                                                            
                                                            <a data-toggle="modal" gig-id="<%= gigs[i].unique_id %>" class="btn btn-outline-success" href="#myModullUNBLOCKGIG"><i class="ti-unlock"></i></a>
                                                            <% } %>
                                                            <a data-toggle="modal" gig-id="<%= gigs[i].unique_id %>" class="btn btn-danger" href="#myModullDELT"><i class="c-red-500 ti-trash"></i></a>
                                                        
                                                        </div> 
                                                        </td>
                                                    </tr>
                                                    <% } %>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                
                
                <!-- Modal -->
		    <div aria-hidden="true" aria-labelledby="myModalLabel" role="dialog" tabindex="-1" id="myModullBLOCKGIG" class="modal fade">
		       <div class="modal-dialog">
		         <div class="modal-content">
		           <div class="modal-header">
		             <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
		               <h4 class="modal-title"> Block Gig?</h4>
		           </div>
		       <div class="modal-body">
		       <p>Are you sure you want to Block this Gig?</p>
		       </div>
               <div class="modal-footer">
                   <form id="ct-block-gig-form">
                       <button data-dismiss="modal" class="btn btn-success" type="button">Cancel</button>
                       <button class="btn btn-success ct-block-this-gig" >Block</button>
                   </form>
               </div>
		          </div>
		              </div>
		          </div>
<!-- modal --> 
               
                 <!-- Modal -->
		    <div aria-hidden="true" aria-labelledby="myModalLabel" role="dialog" tabindex="-1" id="myModullUNBLOCKGIG" class="modal fade">
		       <div class="modal-dialog">
		         <div class="modal-content">
		           <div class="modal-header">
		             <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
		               <h4 class="modal-title"> Unblock Gig?</h4>
		           </div>
		       <div class="modal-body">
		       <p>Are you sure you want to Unblock this Gig?</p>
		       </div>
               <div class="modal-footer">
                   <form id="ct-unblock-gig-form">
                       <button data-dismiss="modal" class="btn btn-success" type="button">Cancel</button>
                       <button class="btn btn-success ct-unblock-this-gig" >Unblock</button>
                   </form>
               </div>
		          </div>
		              </div>
		          </div>
<!-- modal -->
				
					
                 <!-- Modal -->
		    <div aria-hidden="true" aria-labelledby="myModalLabel" role="dialog" tabindex="-1" id="myModullDELT" class="modal fade">
		       <div class="modal-dialog">
		         <div class="modal-content">
		           <div class="modal-header">
		             <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
		               <h4 class="modal-title"> Delete Gig?</h4>
		           </div>
		       <div class="modal-body">
		       <p>Are you sure you want to Delete this Gig?</p>
		       </div>
               <div class="modal-footer">
                   <form id="ct-delete-gig-form">
                       <button data-dismiss="modal" class="btn btn-success" type="button">Cancel</button>
                       <button class="btn btn-success ct-delete-this-gig" >Delete</button>
                   </form>
               </div>
		          </div>
		              </div>
		          </div>
<!-- modal -->
					
                <!-- Content Wrapper END -->

                <!-- Footer START -->
                <footer class="content-footer">
                    <div class="footer">
                        <div class="copyright">
                            <span>Copyright Â© 2017 <b class="text-dark">Theme_Nate</b>. All rights reserved.</span>
                            <span class="go-right">
									<a href="#" class="text-gray mrg-right-15">Term &amp; Conditions</a>
									<a href="#" class="text-gray">Privacy &amp; Policy</a>
								</span>
                        </div>
                    </div>
                </footer>
                <!-- Footer END -->
