

<!-- Content Wrapper START -->
<div class="main-content">
        <div class="container-fluid">
                <div class="row">
                <div class="col-lg-3">
                            <div class="widget-profile-1 card" style="border: solid 1px #454545;">
                                <div class="profile border bottom xt-img-box">
                                    <div class="xt-cover-img">
                                            <img style="border-radius: 0%;" class="mrg-top-30" src="/default/uploads/gig-cover/<%= gigDetails[0].cover_image %>" alt="">
    
                                    </div>
                                    <h4 class="mrg-top-20 no-mrg-btm text-semibold">
                                            <%= gigDetails[0].title %> 
                                    </h4>
                                </div>
                                <div class="pdd-horizon-30 pdd-vertical-20">
                                    <h5 class="text-semibold mrg-btm-5">Details</h5>
                                    <br />
                                    
                                    <!-- <span><i class="ti-user" style="color: #D90003;"></i> Ihediobi Dominic</span>
                                    <br />
                                    <br /> -->
                                    
                                    <span>
                                        <!-- <i class="ti-email" style="color: #00E813;"></i>  -->
                                        <p > 
                                            <span style="color: #515365;font-size:13px;">Main Category:</span>
                                            <%= catTitle[0].title %><p/>
                                        
                                    </span>
                                    
                                    <span> 
                                        <!-- <i class="ti-calendar" style="color: #000BE4;"></i>  -->
                                        <p > <span style="color: #515365;font-size:13px;">Sub Category:</span>
                                            <%= catTitle[1].title %><p/>
                                        
                                    </span>
                                    
                                    <span> 
                                        <!-- <i class="ti-mobile" style="color: #D901E7;"></i>  -->
                                        <p > <span style="color: #515365;font-size:13px;">Reviews:</span>
                                            <%= gigDetails[0].reviews %> <p/>
                                        
                                    </span>
                                    
                                </div>
                            </div>
                        </div>
                        
                        <div class="col-md-9">
                    <div class="card" style="border: 1px solid #454545;">
                        <div class="card-block">
                            <div class="row">
                                <div class="col-md-12">
                                    <h4 class="card-title">User Details</h4>
                                    <form id="ct-update-gig-details-form" class="form-horizontal mrg-top-40 pdd-right-30">
                                        <div class="form-group row">
                                            <label class="col-md-2 control-label">Title:</label>
                                            <div class="col-md-10">
                                                <input type="hidden" id="form-1-3" name="gig_id" value="<%= gigDetails[0].unique_id %>">
                                                <input type="text" class="form-control" id="form-1-1" placeholder="Full Name" name="title" value="<%= gigDetails[0].title %>">
                                            </div>
                                        </div>
                                        <div class="form-group row">
                                            <label  class="col-md-2 control-label">Email Address:</label>
                                            <div class="col-md-10">
                                                <input type="email" class="form-control"placeholder="Email" value="<%= gigDetails[0].email %>" disabled>
                                            </div>
                                        </div>
                                        <div class="form-group row">
                                            <label class="col-md-2 control-label">Main Category:</label>
                                            <div class="col-md-10">
                                                <input type="text" class="form-control" placeholder="Email" value="<%= catTitle[0].title %>" disabled>
                                            </div>
                                        </div>
                                        <div class="form-group row">
                                            <label class="col-md-2 control-label">Sub Category:</label>
                                            <div class="col-md-10">
                                                <input type="text" class="form-control"placeholder="Email" value="<%= catTitle[1].title %>" disabled>
                                            </div>
                                        </div>
                                        <button class="btn btn-primary btn-sm xt-update-gig-btn" style="float:right;">Update this Gig</button>
                                    </form>
                                </div>
                                
                            </div>
                        </div>
                    </div>
                </div>
                
                
            </div>
        </div>
    </div>
    <!-- Content Wrapper END -->
    
