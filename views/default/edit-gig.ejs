<!-- Wrapper Start -->
<div id="wt-wrapper" class="wt-wrapper wt-haslayout">
        <!-- Content Wrapper Start -->
        <div class="wt-contentwrapper">
            <!-- Header Start -->
            
            <!--Header End-->
            <!--Main Start-->
            <main id="wt-main" class="wt-main wt-haslayout custom-main">
                    
                    <!--Sidebar Start-->
                    <div id="wt-sidebarwrapper" class="wt-sidebarwrapper">
                            <div id="wt-btnmenutoggle" class="wt-btnmenutoggle">
                                <span class="menu-icon">
                                    <em></em>
                                    <em></em>
                                    <em></em>
                                </span>
                            </div>
                            <div id="wt-verticalscrollbar" class="wt-verticalscrollbar">
                                <div class="wt-companysdetails wt-usersidebar">
                                    <figure class="wt-companysimg">
                                        <img src="/default/images/sidebar/img-01.jpg" alt="img description">
                                    </figure>
                                    <div class="wt-companysinfo">
                                            <figure><img src="/default/uploads/profile-images/<%= user[0].profile_image %> " alt="img description"></figure>
                                        <div class="wt-title">
                                            <h2><a href="javascript:void(0);"> <%= user[0].firstName %> <%= user[0].lastName %></a></h2>
                                            <span>Amento Tech</span>
                                        </div>
                                        <!-- <div class="wt-btnarea"><a href="dashboard-postjob.html" class="wt-btn">Post a Job</a></div> -->
                                    </div>
                                </div>
                                <nav id="wt-navdashboard" class="wt-navdashboard">
                                    <ul>
                                        <li class="wt-active">
                                                <a href="/my-profile">
                                                    <i class="ti-briefcase"></i>
                                                    <span>View Profile</span>
                                                </a>
                                            </li>
                                        <li class="wt-active">
                                            <a href="/Mynotifications">
                                                <i class="ti-dashboard"></i>
                                                <span>My Notifications</span>
                                            </a>
                                        </li>
                                        <li class="wt-active">
                                            <a href="/user">
                                                <i class="ti-package"></i>
                                                <span>Dashboard</span>
                                            </a>
                                        </li>
                                        <li class="wt-notificationicon menu-item-has-children">
                                            <a href="javascript:void(0);">
                                                <i class="ti-pencil-alt"></i>
                                                <span>Gigs</span>
                                            </a>
                                            <ul class="sub-menu">
                                                <li><hr><a href="/createGig">Create Gigs</a></li>
                                                <li><hr><a href="/View-Gigs">Progress On Gigs</a></li>
                                            </ul>
                                        </li>
                                        <li class="wt-active">
                                            <a href="/user_support">
                                                <i class="ti-layers"></i>
                                                <span>Help & Support</span>
                                            </a>
                                        </li>
                                        <li class="wt-active">
                                            <a href="/logout">
                                                <i class="ti-key"></i>
                                                <span>Logout</span>
                                            </a>
                                    </ul>
                                </nav>
                            </div>
                        </div>
                    <!--Sidebar Start-->
                <!--Register Form Start-->
                <section style="margin-top:-80px;" class="wt-haslayout">
                    <div class="row">
                        
                            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-9 col-xl-9">
                                    <div class="wt-haslayout wt-dbsectionspace">
                                        <div class="wt-dashboardbox wt-dashboardtabsholder">
                                            <div class="wt-dashboardboxtitle">
                                                <h2>EDIT THIS GIG</h2>
                                                
                                                    <% if( gig[0].blocked == '1'){ %>
                                                        <p style="padding-top:8px;font-size:12px;margin-bottom: 0px;color: rgb(1, 209, 105);">
                                                            This Gig is currently active
                                                        </p>
                                                    <% }else{ %>
                                                        <p style="padding-top:8px;font-size:12px;margin-bottom: 0px;color: #ff5851;">
                                                            This Gig is currently Inactive
                                                        </p>
                                                    <% } %>
                                                    
                                                
                                            </div>
                                            <div class="wt-dashboardtabs">
                                                <ul class="wt-tabstitle nav navbar-nav">
                                                    <li class="nav-item">
                                                        <a class="active" data-toggle="tab" href="#wt-skills">Overview</a>
                                                    </li>
                                                    <li class="nav-item">
                                                        <a data-toggle="tab" href="#wt-awards">
                                                            Gig Cover Image

                                                        </a>
                                                    </li>
                                                </ul>
                                            </div>
                                            <div class="wt-tabscontent tab-content">
                                                <div class="wt-personalskillshold tab-pane active fade show" id="wt-skills">
                                                    <div class="wt-yourdetails wt-tabsinfo">
                                                        <div class="wt-tabscontenttitle">
                                                            <h2>GIG TITLE</h2>
                                                        </div>
                                                        <form id="edit-gig-form"  class="wt-formtheme wt-userform">
                                                            <fieldset>
                                                                    <div class="form-group">
                                                                        <input id="gig-id" type="hidden" value="<%= gig[0].unique_id %>">
                                                                        <input id="edit-gig-title" type="text" name="edit-title" class="form-control" placeholder="I will..." value="<%= gig[0].title %>"><br>
                                                                        <p style="padding-top:8px;font-size:12px;">Start your Gig Title with: I will... </p>
                                                                    </div>
                                                            </fieldset>
                                                        </form>
                                                    </div>
                                                    <div class="wt-profilephoto wt-tabsinfo">
                                                        <div class="wt-tabscontenttitle">
                                                            <h2>GIG CATEGORY</h2>
                                                        </div>
                                                        <div class="wt-profilephotocontent">
                                                            
                                                        <form id="create-new-gig-category" class="wt-formtheme wt-userform">
                                                                <fieldset>
                                                                    <div class="form-group form-group-half">
                                                                        <span class="wt-select">
                                                                            <select id="edit-gig-maincategory">
                                                                                <option value="" >Select Main Category</option>
                                                                                <% for(var i=0; allGigs[i]; i++){ %>
                                                                                <option 
                                                                                <% if( allGigs[i].unique_id == gig[0].main_category){ %> selected="selected" <% }else{} %> value="<%=allGigs[i].unique_id%>"><%= allGigs[i].title %> 
                                                                                </option>

                                                                                <% } %>
                                                                            </select>
                                                                        </span>
                                                                    </div>
                                                                    <div class="form-group form-group-half">
                                                                        <p id="xt-gig-subcat-id" style="display:none"><%= gig[0].sub_category %></p>
                                                                        <span class="wt-select">
                                                                            <select id="edit-gig-subcategory">
                                                                                <option >Select a Sub Category</option>
                                                                                
                                                                            </select>
                                                                        </span>
                                                                    </div>
                                                                </fieldset>
                                                            </form>
                                                        </div>
                                                    </div>
                                                    <div class="form-group wt-btnarea">
                                                            <button class="wt-btn edit-gig">Edit & Continue</button>
                                                        </div>
                                                </div>
                                                
                                    <div class="wt-awardsholder tab-pane fade" id="wt-awards">
                                            <div class="wt-addprojectsholder wt-tabsinfo">
                                                <div class="wt-tabscontenttitle wt-addnew">
                                                    <h2>ADD COVER IMAGE TO GIG</h2>
                                                </div>
                                                <div class="wt-profilephotocontent">
                                                        <form method="POST" 
                                                        action="/upload-gig-cover-image/<%= gig[0].unique_id %>" class="wt-formtheme wt-formprojectinfo wt-formcategory" enctype="multipart/form-data">
                                                            <fieldset>
                                                                <div class="form-group form-group-label">
                                                                    <div class="wt-labelgroup">
                                                                        <label for="filep">
                                                                            <span class="wt-btn">Select Files</span>
                                                                                <input id="ct-gig-uniq_id" type="text" name="gig_id" value="<%= gig[0].unique_id %>">
                                                                                <br><br>
                                                                            <% if(error){  %>
                                                                            <input type="hidden" id="gig-cover-error" 
                                                                            value="<%=error%>" >
                                                                            <% }else{} %>
                                                                            <input type="file" name="uploadedGigFile" id="filep">
                                                                        </label>
                                                                    </div>
                                                                    <div>
                                                                        &nbsp;
                                                                    </div>
                                                                    <div class="form-group wt-btnarea">
                                                                            <button class="wt-btn " type="submit">Upload Image</button>
                                                                    </div>	
                                                                </div>
                                                            </fieldset>
                                                        </form>
                                                    </div>
                                            </div>									
                                        </div>
                                            </div>
                                            
                                        </div>
                                    </div>
                                </div>
                        <div class="col-xs-12 col-sm-12 col-md-6 col-lg-3 col-xl-3">
                            <div class="wt-haslayout wt-dbsectionspace wt-codescansidebar">
                                <div style="margin-left: 17px;" class="wt-article  gig-box">
                                    <div class="gig-cover-img">
                                                <img src="/default/uploads/gig-cover/<%= gig[0].cover_image %>" alt="img description">
                                    </div>
                                    <div class="wt-articlecontent gig-content">
                                            <div class="wt-title ct-user">
                                                <p><a href="">
                                                        @<%= user[0].firstName %> <%= user[0].lastName %>
                                                </a></p>
                                            </div>
                                        <div class="wt-title gig-main-body">
                                            <p>                                    <%= gig[0].title %>
                                            </p>
                                            <p class="gig-rating">
                                                Rating ( <%= gig[0].reviews %> )
                                            </p>
                                        </div>
                                        <div class="gig-footer">
                                                <!-- <a href="/edit-Gig/<%= gig[0].unique_id %>" class="ct-btn">
                                                    <i title="Edit button" style="font-size:20px;" class="fa fa-pencil" aria-hidden="true"></i>
                                                    Edit ?
                                                </a> -->
                                                 <!-- <button style="float:right;" href="" class="ct-btn">
                                                    <i title="Delete button" style="font-size:20px;" class="fa fa-trash" aria-hidden="true"></i>
                                                   
                                                </button>  -->
                                                <a style="float:right;" data-toggle="modal" gig-id="<%= gig[0].unique_id %>"href="#myModullUNBLOCKGIG" class="ct-btn xt-delete-btn">
                                                    <i style="font-size:20px;" class="fa fa-trash" aria-hidden="true"></i>
                                                   
                                                </a>
                                        </div>
                                    </div>
                                </div>
                                <div class="wt-companyad">
                                    <figure class="wt-companyadimg"><img src="/default/images/add-img.jpg" alt="img description"></figure>
                                    <span>Advertisement  255px X 255px</span>
                                </div>
                            </div>
                        </div>
                    </div>
                </section>
                <!--Register Form End-->
            </main>
            <!--Main End-->	
            <div aria-hidden="true" aria-labelledby="myModalLabel" role="dialog" tabindex="-1" id="myModullUNBLOCKGIG" class="modal fade">
                <div class="modal-dialog">
                  <div class="modal-content">
                    <div class="modal-header">
                      <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
                        <h4 class="modal-title"> Delete Gig?</h4>
                    </div>
                <div class="modal-body">
                <p>Are you sure you want to Delete this Gig?</p>
                </div>
                <div class="modal-footer">
                    <form id="ct-delete-gig-form">
                        <button data-dismiss="modal" class="btn btn-success" type="button">Cancel</button>
                        <button class="btn btn-success ct-delete-this-gig" >Delete</button>
                    </form>
                </div>
                   </div>
                       </div>
                   </div>